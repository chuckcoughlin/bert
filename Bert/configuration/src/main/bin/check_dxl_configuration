PROG="dxl_read.py"
python3 ${PROG} --port /dev/ttyACM1 --id 11
python3 ${PROG} --port /dev/ttyACM1 --id 12
python3 ${PROG} --port /dev/ttyACM1 --id 13
python3 ${PROG} --port /dev/ttyACM1 --id 14
python3 ${PROG} --port /dev/ttyACM1 --id 15
python3 ${PROG} --port /dev/ttyACM1 --id 21
python3 ${PROG} --port /dev/ttyACM1 --id 22
python3 ${PROG} --port /dev/ttyACM1 --id 23
python3 ${PROG} --port /dev/ttyACM1 --id 24
python3 ${PROG} --port /dev/ttyACM1 --id 25

python3 ${PROG} --port /dev/ttyACM0 --id 31
python3 ${PROG} --port /dev/ttyACM0 --id 32
python3 ${PROG} --port /dev/ttyACM0 --id 33
python3 ${PROG} --port /dev/ttyACM0 --id 34
python3 ${PROG} --port /dev/ttyACM0 --id 35
python3 ${PROG} --port /dev/ttyACM0 --id 36
python3 ${PROG} --port /dev/ttyACM0 --id 37
python3 ${PROG} --port /dev/ttyACM0 --id 41
python3 ${PROG} --port /dev/ttyACM0 --id 42
python3 ${PROG} --port /dev/ttyACM0 --id 43
python3 ${PROG} --port /dev/ttyACM0 --id 44
python3 ${PROG} --port /dev/ttyACM0 --id 51
python3 ${PROG} --port /dev/ttyACM0 --id 52
python3 ${PROG} --port /dev/ttyACM0 --id 53
python3 ${PROG} --port /dev/ttyACM0 --id 54




